Problem1:
{
    var1 : Int = 0;
}
[a : Int, b : Int] = 
            var1 = b*3;
            [-a-(-2), -b*3];

Problem5: 
{
var1 : Int = 0;
var2 : Int = 0;
x : Int = 0;
}
[a : Int] = x = var1 + var2 + a;
               [var1 + var2 + a, a];
               var1 = var2;
	           var2 = x;

